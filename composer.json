{
  "name": "ethical-jobs/laravel-storage",
  "description": "Laravel repository storage package",
  "license": "MIT",
  "require": {
    "php": "~8.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.5",
    "fakerphp/faker": "^1.19",
    "orchestra/testbench": "*",
    "orchestra/database": "*",
    "laravel/legacy-factories": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "EthicalJobs\\Storage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
